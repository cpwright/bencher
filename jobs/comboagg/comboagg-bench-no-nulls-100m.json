{
  "benchmarks": [
    {
      "title": "Do a complex ComboAgg on 100m rows",
      "generator_files": [ "relation-no-nulls-100m.json" ],
      "benchmark": {
          "statements" : [
              {
                  "title" : "preamble",
                  "text" : [
                      "import time",
                      "import deephaven.ParquetTools as pt",
                      "from deephaven import ComboAggregateFactory as caf",
                      "tag = 'no-nulls-100m'",
                      "bench_name = 'comboagg-bench-' + tag",
                  ],
                  "timed" : 0
              },

              {
                  "title" : "load tables",
                  "text" : "relation = pt.readTable('/data/relation-' + tag + '.parquet').view('animal_id', 'adjective_id')",
                  "timed" : 1
              },

              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform ComboAgg",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.by(caf.AggCombo(\\",
                      "  caf.AggSum('SumAdj=adjective_id'),\\",
                      "  caf.AggMin('MinAdj=adjective_id'),\\",
                      "  caf.AggMax('MaxAdj=adjective_id'),\\",
                      "  caf.AggAvg('AvgAdj=adjective_id'),\\",
                      "  caf.AggStd('StdAdj=adjective_id'),\\",
                      "  caf.AggFirst('FirstAdj=adjective_id'),\\",
                      "  caf.AggLast('LastAdj=adjective_id'),\\",
                      "  caf.AggCount('CountAdj')\\",
                      "), 'animal_id')",
                      "time_end_ns = time.perf_counter_ns()",
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
    }
  ]
}
